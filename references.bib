@article{huang_treesummarizedexperiment_2021,
	title = {{TreeSummarizedExperiment}: a S4 class for data with hierarchical structure},
	volume = {9},
	issn = {2046-1402},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC7684683/},
	doi = {10.12688/f1000research.26669.2},
	shorttitle = {{TreeSummarizedExperiment}},
	abstract = {Data organized into hierarchical structures (e.g., phylogenies or cell types) arises in several biological fields. It is therefore of interest to have data containers that store the hierarchical structure together with the biological profile data, and provide functions to easily access or manipulate data at different resolutions. Here, we present {TreeSummarizedExperiment}, a R/S4 class that extends the commonly used {SingleCellExperiment} class by incorporating tree representations of rows and/or columns (represented by objects of the phylo class). It follows the convention of the {SummarizedExperiment} class, while providing links between the assays and the nodes of a tree to allow data manipulation at arbitrary levels of the tree. The package is designed to be extensible, allowing new functions on the tree (phylo) to be contributed. As the work is based on the {SingleCellExperiment} class and the phylo class, both of which are popular classes used in many R packages, it is expected to be able to interact seamlessly with many other tools.},
	pages = {1246},
	journaltitle = {F1000Research},
	shortjournal = {F1000Res},
	author = {Huang, Ruizhu and Soneson, Charlotte and Ernst, Felix G.M. and Rue-Albrecht, Kevin C. and Yu, Guangchuang and Hicks, Stephanie C. and Robinson, Mark D.},
	urldate = {2024-07-15},
	date = {2021-03-02},
	note = {tex.eprint: 33274053
tex.eprinttype: pmid
tex.pmcid: {PMC}7684683},
	file = {Huang et al_2021_TreeSummarizedExperiment:/home/artur/Zotero/storage/L7964BLL/Huang et al_2021_TreeSummarizedExperiment.pdf:application/pdf;Huang et al_2021_TreeSummarizedExperiment.pdf:/home/arsann/Documents/00-09_Meta/02_zotero/University of Turku/PhD/2023_research_plan_salaried_position/Huang et al_2021_TreeSummarizedExperiment.pdf:application/pdf;Huang et al_2021_TreeSummarizedExperiment.pdf:/home/arsann/Documents/00-09_Meta/02_zotero/University of Turku/PhD/2023_research_plan_salaried_position/Huang et al_2021_TreeSummarizedExperiment2.pdf:application/pdf;Huang et al_2021_TreeSummarizedExperiment.pdf:/home/artur/Documents/00-09_Meta/02-zotero/University of Turku/PhD/2023_research_plan_salaried_position/Huang et al_2021_TreeSummarizedExperiment.pdf:application/pdf;huangTreeSummarizedExperimentS4Class2021-zotero.md:/home/arsann/huangTreeSummarizedExperimentS4Class2021-zotero.md:text/markdown},
